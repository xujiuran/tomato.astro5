{
  "compilerOptions": {
    "baseUrl": ".",
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ],
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@images/*": [
        "src/images/*"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
  }
}